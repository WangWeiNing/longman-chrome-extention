{
  "name": "Longman Dictionary Bubble",
  "short_name": "Lmd bubble",
  "description": "Search word from Longman Dictionary of Contemporary English (5th edition)",
  "author": "nator333",
  "version": "1.0.1",
  "icons": {
    "16": "icons/16.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "permissions": [
    "activeTab",
    "storage"
  ],
  "browser_action": {
    "default_title": "title",
    "default_popup": "pages/popup.html"
  },
  "options_page": "pages/options.html",
  "content_scripts": [
    {
      "js": [
        "js/content.js"
      ],
      "css": [
        "css/content.css"
      ],
      "run_at": "document_end",
      "matches": [
        "<all_urls>"
      ],
      "all_frames": true
    }
  ],
  "manifest_version": 2,
  "content_security_policy": "script-src 'self'; object-src 'self'"
}
