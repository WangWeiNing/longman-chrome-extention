{
  "name": "__MSG_extName__",
  "description": "__MSG_extDescription__",
  "author": "nator333 <m.nakamata35@gmail.com>",
  "version": "1.0.0",
  "icons": {
    "16": "icons/16.ico",
    "48": "icons/48.ico",
    "128": "icons/128.ico"
  },
  "permissions": [
    "activeTab",
    "contextMenus",
    "storage"
  ],
  "browser_action": {
    "default_title": "title",
    "default_popup": "pages/popup.html"
  },
  "options_page": "pages/options.html",
  "content_scripts": [
    {
      "js": [
        "js/content.js"
      ],
      "css": [
        "css/index.css"
      ],
      "run_at": "document_end",
      "matches": [
        "<all_urls>"
      ],
      "all_frames": true
    }
  ],
  "manifest_version": 2,
  "content_security_policy": "script-src 'self'; object-src 'self'"
}
